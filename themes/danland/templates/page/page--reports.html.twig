{# #}

{% if is_front %}
{% endif %}

<div class="row cover">
     {{ page.slideshow }}
</div>

{% if(cover_image) %}
    <section class="row cover" style="background-image: url({{ cover_image }});"></section>
{% else %}
    <section class="row " style="height: 70px;"></section>
{% endif %}

<section class="row nav">
	<div class="row content">
        <div class="row nav">
            <div class="row heading"><h5>Navigate This Section</h5></div>
            <div class="row data">
                <ul>
                <li class="active parent"><a href="">Parent</a>
                    <ul>
                    <li><a href="">Child</a></li>
                    <li><a href="">Child</a></li>
                    <li><a href="">Child</a></li>
                    <li><a href="">Child</a></li>
                    <li><a href="">Child</a></li>
                    </ul>
                </li>
                <li><a href="">Link</a></li>
                <li><a href="">Link</a></li>
                <li><a href="">Link</a></li>
                <li><a href="">Link</a></li>
                </ul>
            </div>
        </div>
    </div>
</section><!-- row.nav -->

{% if page.content %}
	<section class="row main reports sidebar">
		<div class="row content">
			<div class="col prime">
				<div class="row box">
					
                    {{ title_prefix }}
                        {% if title %}
                          <div class="row heading"><h2>{{ title }}</h2></div>
                        {% endif %}
                    {{ title_suffix }}

					<div class="row bread">
						{{ page.breadcrumb }}
					</div>
					<div class="row entry">{{ page.content }}</div>
				</div>
			</div>
			<div class="col aside">
				<div class="row nav">{{ page.sidebar_menu }}</div>
				
				<div class="row resources">{{ page.additional_resources }}</div>
			</div><!-- col.aside -->
		</div><!-- row.content -->
	</section>
{% endif %}